<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="martijnmanual003" author="martijn">

        <loadData encoding="UTF-8"
                  file="config/liquibase/datasources.csv"
                  separator="|"
                  tableName="T_DATASOURCE"/>
                                    
        <loadData encoding="UTF-8"
                  file="config/liquibase/tracks.csv"
                  separator="|"
                  tableName="T_TRACK"/>

        <loadData encoding="UTF-8"
                  file="config/liquibase/datasource_track.csv"
                  separator="|"
                  tableName="T_DATASOURCE_T_TRACK"/>

	</changeSet>
	
	<changeSet id="martijnmanual004" author="martijn">
   		<update tableName="T_DATASOURCE">
    	    <column name="IRI" value="${triplestore.localdatasource}">
        	</column>
        	<where>IRI='DSNAME_PLACEHOLDER'</where>
	    </update>
	</changeSet>


</databaseChangeLog>